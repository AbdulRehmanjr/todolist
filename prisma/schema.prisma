generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Task {
    taskId   String  @id @default(uuid())
    taskName String
    isDone   Boolean @default(false)
}

model Student {
    userId    String  @id @default(uuid())
    name      String
    age       Int
    phone     String
    address   String
    isPresent Boolean @default(true)
}
